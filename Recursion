//Recursion 
#include<stdio.h>
#include<limits.h>
int minCoins(int a[], int n, int sum)
{
    if (sum == 0) 
        return 0;

    int c = INT_MAX;

    for (int i=0; i<n; i++)
    {
	    if (a[i] <= sum)
	    {
		    int result = minCoins(a, n, sum-a[i]);

		    if (result != INT_MAX && result + 1 < c)
			    c = result + 1;
	    }
    }
    return c;
}

int main()
{
	int n,sum;
	scanf("%d",&n);
	int a[n];
	for(int i=0;i<n;i++)
	    scanf("%d",&a[i]);
	scanf("%d",&sum);
	printf("Coins: %d",minCoins(a, n, sum));
	return 0;
}

